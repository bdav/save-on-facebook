{
	"manifest_version": 2,

	"name": "Save on Facebook",
	"description": "Saves the current page to your saved posts on facebook.",
	"version": "1.0",

	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html",
		"default_title": "Click here!"
	},

	"permissions": [
		"activeTab",
		"https://ajax.googleapis.com/"
	]
}